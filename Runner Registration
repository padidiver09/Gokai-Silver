let raceNumber = Math.floor(Math.random() * 1000);
let earlyRegister = false;
let runnerAge = 29;

raceNumber ? (earlyRegister && runnerAge > 18) : raceNumber += 1000;

if (earlyRegister && runnerAge > 18) {
  console.log(`Your race will start at 9:30. Your race number is ${raceNumber}.`);
} else if (!earlyRegister && runnerAge >18) {
  console.log(`Late adults run at 11:00 am. Your race number is ${raceNumber}.`);
} else if  (runnerAge < 18) {
  console.log('Youth registrants run at 12:30 pm (regardless of registration)');
   } else { console.log('Please see the registration desk');
 };
